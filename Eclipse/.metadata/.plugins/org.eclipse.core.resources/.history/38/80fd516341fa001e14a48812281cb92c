package com.dan.learnspringframework.game;

public class GameRunner {
//	MarioGame marioGame;
	SuperContra game;

	public GameRunner(SuperContra game) {
		this.game = game;
	}

	public void run() {
		System.out.println("Running " + game);
		game.up();
		game.down();
		game.left();
		game.right();
	}

}
